<?xml version="1.0"?>
<robot name="advanced_differential_robot">

  <material name="blue">
    <color rgba="0 0 0.8 1"/>
  </material>
  <material name="black">
    <color rgba="0.1 0.1 0.1 1"/>
  </material>
  <material name="gray">
    <color rgba="0.5 0.5 0.5 1"/>
  </material>
  <material name="dark_gray">
    <color rgba="0.3 0.3 0.3 1"/>
  </material>

  <link name="base_link">
    <visual>
      <geometry>
        <box size="1.0 0.5 0.15"/>
      </geometry>
      <material name="blue"/>
    </visual>
    <collision>
      <geometry>
        <box size="1.0 0.5 0.15"/>
      </geometry>
    </collision>
    <inertial>
      <mass value="12.0"/>
      <inertia ixx="0.4" ixy="0.0" ixz="0.0" iyy="0.4" iyz="0.0" izz="0.3"/>
    </inertial>
  </link>

  <link name="body_link">
    <visual>
      <geometry>
        <box size="0.4 0.3 0.8"/>
      </geometry>
      <material name="gray"/>
    </visual>
    <collision>
      <geometry>
        <box size="0.4 0.3 0.8"/>
      </geometry>
    </collision>
    <inertial>
      <mass value="6.0"/>
      <inertia ixx="0.2" ixy="0.0" ixz="0.0" iyy="0.2" iyz="0.0" izz="0.4"/>
    </inertial>
  </link>

  <joint name="body_joint" type="fixed">
    <parent link="base_link"/>
    <child link="body_link"/>
    <origin xyz="0 0 0.475" rpy="0 0 0"/>
  </joint>

  <link name="screen_link">
    <visual>
